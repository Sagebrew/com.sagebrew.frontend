<div class="row">
  <div class="col-md-9">
    <h4>News</h4>
    {{#each model.news.results as |result|}}
      <div class="news-item">
        <h4>{{result.title}}</h4>
        <p>{{result.content}}</p>
      </div>

    {{/each}}
  </div>
  <div class="col-md-3">
    <h4>Sidebar</h4>
  </div>
</div>
